{
    "name": "foodflare-auth",
    "version": "1.0.6",
    "description": "A food-finder API authorizer designed for the FoodFlare app",
    "main": "index.js",
    "private": true,
    "scripts": {
        "create": "claudia create --name foodflareAuth --version dev --region us-east-1 --handler index.authChecker --profile foodflare",
        "update": "claudia update --version dev --profile foodflare",
        "set-version": "claudia set-version --version prod --profile foodflare",
        "destroy": "claudia destroy --profile foodflare",
        "test": "jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/cbnventures/foodflare-auth.git"
    },
    "funding": {
        "type": "paypal",
        "url": "https://www.paypal.me/cbnventures"
    },
    "keywords": [
        "foodflare",
        "food",
        "api",
        "claudia",
        "aws"
    ],
    "author": "CBN Ventures LLC",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/cbnventures/foodflare-auth/issues"
    },
    "files": [
        ".env",
        "*.js",
        "certs/*.pem",
        "src/**/*.js"
    ],
    "homepage": "https://github.com/cbnventures/foodflare-auth#readme",
    "engines": {
        "node": ">=12.0.0",
        "claudia": ">=5.12.0"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.44",
        "@types/node": "^13.7.4",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.20.0",
        "aws-lambda": "^1.0.5",
        "claudia": "^5.12.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jasmine": "^4.1.0",
        "jasmine": "^3.5.0",
        "typescript": "^3.8.2"
    }
}
